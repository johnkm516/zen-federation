{
  "name": "zen",
  "version": "0.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "nx serve portal",
    "start:auth": "nx serve auth-api --verbose",
    "start:products": "nx serve products --verbose",
    "start:e2e": "nx e2e portal-e2e --watch",
    "frontend:gen": "graphql-codegen -c apollo-angular.yml && graphql-codegen -c apollo-possible-types.yml",
    "api:gen": "prisma generate --schema=./libs/prisma-clients/Auth/prisma/schema.prisma && ts-node generate.ts",
    "prisma:migrate": "prisma migrate dev --schema=./libs/prisma-clients/Auth/prisma/schema.prisma && prisma migrate dev --schema=./libs/prisma-clients/Products/prisma/schema.prisma && ts-node generate.ts",
    "prisma:migrate:Auth": "prisma migrate dev --schema=./libs/prisma-clients/Auth/prisma/schema.prisma && ts-node generate.ts",
    "prisma:migrate:Products": "prisma migrate dev --schema=./libs/prisma-clients/Products/prisma/schema.prisma && ts-node generate.ts",
    "prisma:create-only": "prisma migrate dev --create-only",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prod:apps": "nx build portal --prod",
    "prod:api": "nx build api --prod && npm run dockerize:api",
    "kompose": "kompose convert -f docker-compose.yml --out ./deploy/k8s/",
    "compose:up": "ts-node tools/create-deploy-package.ts && docker compose up -d --build && docker compose restart pgadmin",
    "kube:deploy": "kubectl apply -f ./deploy/k8s/",
    "dockerize:api": "ts-node tools/create-deploy-package.ts && docker build -f deploy/api/Dockerfile -t zen-api .",
    "docker:run-api": "docker run -d --name zen-api -p 7080:7080 zen-api",
    "docs": "ng compodoc api",
    "test": "nx run-many --all --target=test --parallel",
    "lint": "nx run-many --all --target=lint --parallel",
    "format": "nx format:write --all",
    "build": "nx build portal",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main"
  },
